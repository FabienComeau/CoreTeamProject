@model IEnumerable<CoreTeamProject.Models.Events>
<!-- Blog Content
================================================== -->
<div class="row">

            <!-- Blog Posts
            ================================================== -->
            <div class="span12 blog">

                <a asp-action="Create">Create New</a>
                </p>
                <table class="table">
                    <thead>
                        <tr>
                            <th>
                                @Html.DisplayNameFor(model => model.eventName)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.eventDescription)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.eventDate)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.eventTime)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.eventLocation)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.eventEmail)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.eventPhoneNumber)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.eventCost)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Subcategory)
                            </th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => item.eventName)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.eventDescription)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.eventDate)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.eventTime)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.eventLocation)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.eventEmail)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.eventPhoneNumber)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.eventCost)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Subcategory.subCategoryID)
                                </td>
                                <td>
                                    <a asp-action="Edit" asp-route-id="@item.eventID">Edit</a> |
                                    <a asp-action="Details" asp-route-id="@item.eventID">Details</a> |
                                    <a asp-action="Delete" asp-route-id="@item.eventID">Delete</a>
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>


                <!-- Pagination -->
                <div class="pagination">
                    <ul>
                        <li class="active"><a href="#">Prev</a></li>
                        <li class="active"><a href="#">1</a></li>
                        <li><a href="#">2</a></li>
                        <li><a href="#">3</a></li>
                        <li><a href="#">4</a></li>
                        <li><a href="#">Next</a></li>
                    </ul>
                </div>
            </div>
    </div>

